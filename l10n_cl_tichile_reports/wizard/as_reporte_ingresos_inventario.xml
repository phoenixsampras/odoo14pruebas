<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="as_reporte_ingreso_inventario" model="ir.ui.view">
            <field name="name">Reporte listado de Productos</field>
            <field name="model">as.ingreso.inventario</field>
            <field name="arch" type="xml">
                <form string="ingresos de Inventario">
                    <group col="4">
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <!-- <field name="partner_ids" widget="many2many_tags" options="{&quot;always_reload&quot;: True, 'no_open': True,'no_create': 1, 'no_create_edit': 1}"/> -->
                        <field name="location_ids" widget="many2many_tags" options="{&quot;always_reload&quot;: True, 'no_open': True,'no_create': 1, 'no_create_edit': 1}"/>
                        <field name="user_ids" widget="many2many_tags" options="{&quot;always_reload&quot;: True, 'no_open': True,'no_create': 1, 'no_create_edit': 1}"/>
                        <field name="product_id" widget="many2many_tags" options="{&quot;always_reload&quot;: True, 'no_open': True,'no_create': 1, 'no_create_edit': 1}"/>
                    </group>
                    <footer>
                        <button name="export_xls" 
                            type="object" 
                            default_focus="1"
                            string="Exportar Excel" 
                            context="{'xls_export':1}"
                            icon="fa-print"
                            class="oe_highlight"/>
                          <!-- <button name="export_pdf" 
                            type="object" 
                            default_focus="1"
                            string="Exportar PDF" 
                            context="{'xls_export':1}"
                            icon="fa-print"
                            class="oe_highlight"/> -->
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="as_action_ingreso_inventario" model="ir.actions.act_window">
            <field name="name">ingresos de Inventario action</field>
            <field name="res_model">as.ingreso.inventario</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="as_reporte_ingreso_inventario"/>
            <field name="target">new</field>
        </record>
        
      
    
        <menuitem id="as_ingresos_inventario_id" name="Ingresos de Inventario" action="as_action_ingreso_inventario" parent="stock.menu_warehouse_report"   sequence="12"/>
    </data>
</odoo>